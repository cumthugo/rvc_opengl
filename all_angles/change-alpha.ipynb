    "import os\n",
    "import cv2\n",
    "list = os.listdir('.')\n",
    "print(list)\n",
    "for l in list:\n",
    "    if l.endswith('png'):\n",
    "        rvc = cv2.imread(l,cv2.IMREAD_UNCHANGED)\n",
    "        rvc[:,:,3] = rvc[:,:,0]\n",
    "        cv2.imwrite('alpha-'+ l, rvc)\n",
    "        print(f'processed {l}')"
   ]
